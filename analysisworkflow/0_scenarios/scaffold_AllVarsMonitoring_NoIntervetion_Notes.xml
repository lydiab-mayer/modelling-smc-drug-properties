  <healthSystem>
    <ImmediateOutcomes name="Tanzania ACT"> <!-- might be a sufficient default assumption; change only for adherence but can do it via access -->
      <drugRegimen firstLine="ACT" inpatient="QN" secondLine="ACT"/>
       <initialACR>
            <ACT value="1"/>
            <QN value="1"/>
            <selfTreatment value="1"/>
          </initialACR>
          <compliance>
            <ACT value="1"/>
            <QN value="1"/>
            <selfTreatment value="1"/>
          </compliance>
          <nonCompliersEffective>
            <ACT value="0"/>
            <selfTreatment value="0"/>
          </nonCompliersEffective>
		  <treatmentActions> <!-- assumes if you get treatment you clear infection; might change for iTPP3 -->
        <ACT name="clear blood-stage infections">
          <clearInfections stage="blood" timesteps="1"/>
        </ACT>
        <QN name="clear blood-stage infections">
          <clearInfections stage="blood" timesteps="1"/>
        </QN>
      </treatmentActions>
	  <pSeekOfficialCareUncomplicated1 value="@access@"/> <!-- what matters most in this example because everything else is =1; okay for archetype settings; translate 2 week time period numbers from DHS data to 5d simulation time step; double-check -->
      <pSelfTreatUncomplicated value="0"/>
      <pSeekOfficialCareUncomplicated2 value="@access@"/>
      <pSeekOfficialCareSevere value="0.48"/> <!-- best estimate -->
      <!-- Value 0.48 used in Tediosi et al., 2006 for reference scenario -->
    </ImmediateOutcomes>
    <CFR> <!-- keep the same -->
