      <option name="Vector_Nv" value="true"/>         <!-- ID 32 -->
      <option name="Vector_Ov" value="true"/>         <!-- ID 33 -->
      <option name="Vector_Sv" value="true"/>         <!-- ID 34 -->
      <option name="inputEIR" value="true"/>          <!-- ID 35 --> <!-- check why it is discrete? -->
      <option name="simulatedEIR" value="true"/>      <!-- ID 36 --> <!-- check why it is discrete? -->
