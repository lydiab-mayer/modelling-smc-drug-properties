      <option name="nInfect" value="true"/>           <!-- ID 1 -->
      <option name="nExpectd" value="true"/>          <!-- ID 2 -->
      <option name="nPatent" value="true"/>           <!-- ID 3 --> <!-- based on diagnostic -->
      <option name="totalInfs" value="true"/>         <!-- ID 6 -->
      <option name="nTransmit" value="true"/>         <!-- ID 7 -->
      <option name="totalPatentInf" value="true"/>    <!-- ID 8 -->
