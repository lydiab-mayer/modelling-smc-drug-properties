  <vector>
      <anopheles mosquito="gambiae_ss_indoor" propInfected="0.078" propInfectious="0.021">
        <seasonality annualEIR="@indoor@" input="EIR">     
          <monthlyValues smoothing="fourier">		  
			<value>	@Jan@</value>
			<value>	@Feb@</value>
			<value>	@Mar@</value>
			<value>	@Apr@</value>
